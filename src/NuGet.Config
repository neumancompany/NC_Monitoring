    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" protocolVersion="3" />

    <add key="devexpress" value="https://nuget.devexpress.com/yAvbxl4VimILVfsljH6f5VGglOjjD1SaBeAMipIr2hzRsGasQc/api" />

    <add key="ncnuget" value="https://pkgs.dev.azure.com/veselyncompany/_packaging/ncnuget/nuget/v3/index.json" />

  </packageSources>

  <disabledPackageSources>

  </disabledPackageSources>

</configuration>